{
  "id": null,
  "title": "ML Predictive Maintenance Dashboard",
  "tags": ["ml", "monitoring", "predictive-maintenance"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "üöÄ API Request Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total[2m])",
          "legendFormat": "{{method}} {{endpoint}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 2,
      "title": "üéØ Model Accuracy",
      "type": "stat",
      "targets": [
        {
          "expr": "model_accuracy * 100",
          "legendFormat": "Accuracy"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 95}
            ]
          },
          "unit": "percent",
          "decimals": 1
        }
      }
    },
    {
      "id": 3,
      "title": "‚ö° API Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m])",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "s"
        }
      }
    },
    {
      "id": 4,
      "title": "‚ùå Prediction Errors",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(prediction_errors_total[5m])",
          "legendFormat": "Errors per second"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 5,
      "title": "üìä Total Predictions",
      "type": "stat",
      "targets": [
        {
          "expr": "predictions_total",
          "legendFormat": "Total"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "üü¢ API Health Status",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"predictive-maintenance-api\"}",
          "legendFormat": "API Status"
        }
      ],
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 16},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 1}
            ]
          },
          "mappings": [
            {
              "options": {
                "0": {"text": "DOWN", "color": "red"},
                "1": {"text": "UP", "color": "green"}
              },
              "type": "value"
            }
          ]
        }
      }
    },
    {
      "id": 7,
      "title": "üìà Request Status Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "http_requests_total",
          "legendFormat": "{{status}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 8,
      "title": "üî• Prediction Latency Distribution",
      "type": "histogram",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(prediction_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      }
    }
  ],
  "schemaVersion": 38,
  "version": 1
} 